# Example PyPI configuration file
# Copy this to your home directory as .pypirc
# Windows: C:\Users\YourUsername\.pypirc
# Linux/Mac: ~/.pypirc

[distutils]
index-servers =
    pypi
    testpypi

[pypi]
username = __token__
password = pypi-AgEIcHlwaS5vcmc...YOUR_PYPI_TOKEN_HERE

[testpypi]
repository = https://test.pypi.org/legacy/
username = __token__
password = pypi-AgEIcHlwaS5vcmc...YOUR_TESTPYPI_TOKEN_HERE
